<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="css/custom.css" rel="stylesheet" type="text/css" />
    </head>
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/manaskrasas.jpeg</argument>
                <argument name="logo_alt" xsi:type="string">Custom Design</argument>
                <argument name="logo_img_width" xsi:type="number">20</argument>
                <argument name="logo_img_height" xsi:type="number">20</argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="copyright" remove="true"/>
        
    </body>
</page>

